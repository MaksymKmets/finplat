{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "finplat-ui",
      "image": "maksymkmets/finplat-ui",
      "hostname": "finplat-ui",
      "essential": false,
      "memory": 256,
      "dependsOn": [
        {
          "containerName": "finplat-api",
          "condition": "START"
        }
      ]
    },
    {
      "name": "finplat-api",
      "image": "maksymkmets/finplat-api",
      "hostname": "finplat-api",
      "essential": false,
      "memory": 256
    },
    {
      "name": "nginx",
      "image": "maksymkmets/nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["finplat-ui","finplat-api"],
      "memory": 256,
      "dependsOn": [
        {
          "containerName": "finplat-ui",
          "condition": "START"
        }
      ]
    }
  ]
}